{% extends 'base.html.twig' %}
{% block title %}Créer un Objet {% endblock %}

{% block main %}
    <a href="{{ path('home') }}">Back</a>
    <h2>Création d'un objet</h2>
    {{ form_start(form) }}
    {{ form_errors(form) }}

    {{ form_row(form.name) }}
    {{ form_row(form.cost) }}
    {{ form_row(form.description) }}

    {{ form_row(form.submit, { 'label': 'Générer' }) }}
    {{ form_end(form) }}

{% endblock %}